    <title>Order History</title>
    <link rel="stylesheet" href="/allOrders.css">
</head>
<body class="mainpage">
    <main>
        <% for(let i=0; i<orders.length; i++){%>
            <% let order = orders[i] %>
            <div> 
                <p>Receiver Name: <%= order.receiverName%></p>
                <p>Receiver Address: <%= order.toAddress %>, <%= order.toCity %></p>
                <p>Shipping Price: $<%= order.shippingPrice %></p>
                <p>Product: <%= order.productName %></p>
                <p>Shipping date: <%= order.date %></p>
                <a href="/orders/details/<%= order._id%>">More details</a>
            </div>
        <%}%>

    </main>

